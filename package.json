{
  "name": "fastify-graceful-shutdown",
  "version": "3.2.0",
  "description": "Gracefully shutdown fastify",
  "main": "index.js",
  "scripts": {
    "format": "prettier ./*.js --write",
    "test": "prettier ./*.js --debug-check && tap ./test.js | tap-spec && tsd",
    "test:ts": "tsd",
    "patch": "np patch --yolo",
    "minor": "np minor --yolo",
    "major": "np major --yolo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemerajs/fastify-graceful-shutdown.git"
  },
  "keywords": [
    "web",
    "framework",
    "clean",
    "fastify",
    "graceful",
    "shutdown"
  ],
  "author": "Dustin Deus <deusdustin@gmail.com> (http://www.starptech.de)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hemerajs/fastify-graceful-shutdown/issues"
  },
  "homepage": "https://github.com/hemerajs/fastify-graceful-shutdown#readme",
  "devDependencies": {
    "fastify": "^4.5.3",
    "np": "^7.6.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "tap": "^15.2.3",
    "tap-spec": "^5.0.0",
    "tsd": "^0.23.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "fastify-plugin": "^4.2.1",
    "fastparallel": "^2.4.1"
  }
}
